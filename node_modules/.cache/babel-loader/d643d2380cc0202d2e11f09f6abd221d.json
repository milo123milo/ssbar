{"ast":null,"code":"import News from '../components/News.vue';\nimport { useI18n } from 'vue-i18n';\nimport store from '../store';\nimport { useQuery } from 'villus';\nimport { ref, computed } from 'vue';\nimport { useRoute } from \"vue-router\";\nimport Mainfooter from '../components/mainfooter.vue';\nexport default {\n  data: function () {\n    return {\n      store\n    };\n  },\n  components: {\n    News,\n    Mainfooter\n  },\n\n  setup() {\n    const {\n      t\n    } = useI18n({});\n    const route = useRoute();\n    const id = route.params.id;\n    const naslov = \"Vijesti\" + t('apiN');\n    const sadrzaj = \"Vijesti\" + t('apiS');\n    const vijestis = computed(() => {\n      return `\n      query vijestis {\n vijestis(pagination: {start: 0, limit: 5}, sort: \"id:desc\") {\n  data {\n    id\n    attributes {\n       createdAt\n       VijestiProfilna {\n        data {\n          attributes {\n            url\n          }\n        }\n      }\n      ${naslov}\n      ${sadrzaj}\n      \n    }\n  }\n}\n}\n    `;\n    });\n    const {\n      data\n    } = useQuery({\n      query: vijestis\n    });\n    return {\n      data\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAwDA,OAAOA,IAAP,MAAiB,wBAAjB;AACA,SAASC,OAAT,QAAwB,UAAxB;AACA,OAAOC,KAAP,MAAkB,UAAlB;AACA,SAASC,QAAT,QAAyB,QAAzB;AACA,SAASC,GAAT,EAAcC,QAAd,QAA8B,KAA9B;AACA,SAAQC,QAAR,QAAuB,YAAvB;AACA,OAAOC,UAAP,MAAuB,8BAAvB;AAEA,eAAe;AACXC,MAAI,EAAE,YAAU;AACd,WAAO;AACHN;AADG,KAAP;AAIH,GANY;AAOXO,YAAU,EAAE;AACRT,QADQ;AAERO;AAFQ,GAPD;;AAYXG,OAAK,GAAG;AACR,UAAM;AAAEC;AAAF,QAAQV,OAAO,CAAC,EAAD,CAArB;AACA,UAAMW,KAAI,GAAIN,QAAQ,EAAtB;AACA,UAAMO,EAAC,GAAID,KAAK,CAACE,MAAN,CAAaD,EAAxB;AACA,UAAME,MAAK,GAAI,YAAYJ,CAAC,CAAC,MAAD,CAA5B;AACA,UAAMK,OAAM,GAAI,YAAYL,CAAC,CAAC,MAAD,CAA7B;AACA,UAAMM,QAAO,GAAIZ,QAAQ,CAAC,MAAM;AAC9B,aAAO;;;;;;;;;;;;;;QAcLU,MAAM;QACNC,OAAO;;;;AAIf;AACA;KApBM;AAqBE,KAtBqB,CAAzB;AAwBA,UAAM;AAAER;AAAF,QAAWL,QAAQ,CAAC;AACxBe,WAAK,EAAED;AADiB,KAAD,CAAzB;AAIA,WAAO;AAAET;AAAF,KAAP;AAIF;;AAlDa,CAAf","names":["News","useI18n","store","useQuery","ref","computed","useRoute","Mainfooter","data","components","setup","t","route","id","params","naslov","sadrzaj","vijestis","query"],"sourceRoot":"","sources":["/Users/milo/Documents/Diverse/meteo ui/stbar2/src/views/NewsView.vue"],"sourcesContent":["<template>\n    <div class=\"news\">\n        <div style=\"width: 100%; height: 3vh;\"></div>\n        <div class=\"container-fluid newscont no-padding\">\n            <div class=\"row g-0\">\n                <div class=\"col-xl d-flex justify-content-center\">\n                    <div id=\"npcontain\">\n                    <h1 id=\"nptitle\">{{ $t(\"newsTitle\") }}</h1>\n                    <h1 id=\"npsubtitle\">{{ $t(\"newsSubTitle\") }}</h1>\n                    <div id=\"endline\"></div>\n                    <a class=\"mainarticle\" v-if=\"data\" :href=\"'/article/' + data.vijestis.data[0].id\">\n                    <div id=\"frpost\">\n                        <div id=\"frimg\">\n                    \n                        <img class=\"frimgs\" v-if=\"data\" :src=\"store.baseUrl + data.vijestis.data[0].attributes.VijestiProfilna.data.attributes.url\" alt=\"\">\n                    \n                        </div>\n                        <div class=\"frtext carousel-caption\" v-if=\"data\">{{  data.vijestis.data[0].attributes['Vijesti' + $t('apiN')] }}</div>\n                    </div>\n                    </a>\n                    </div>\n                </div>\n                \n                <div class=\"col-xl rightbar\">\n                    <div class=\"container-fluid containerpadding\" >\n                        <div class=\"hedr\">\n                                <div id=\"semititle\" style=\"font-size: 2.4rem; color: #041F30; width:100%\">{{ $t(\"latestNews\") }}</div>\n                                <a class=\"a\" href=\"/news\">\n                                <div class=\"btnmore\" style=\"\">{{ $t(\"readMoreNews\") }}</div>\n                          