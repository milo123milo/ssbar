em\" :src=\"store.baseUrl + aitem.attributes.VijestiProfilna.data.attributes.url\" alt=\"\">\n        <div class=\"container-fluid p-0\" style=\"height: fit-content;\">\n        <div id=\"newsdate\" class=\"row g-0\" v-if=\"aitem\">{{ formatDate(aitem.attributes.createdAt) }}</div>\n        <div id=\"newsnaslov\" class=\"row g-0\" v-if=\"aitem\">{{ aitem.attributes['Vijesti' + $t('apiN')] }}</div>\n        <div id=\"newscont\" class=\"row g-0\" v-if=\"aitem\">{{ removeMarks(aitem.attributes['Vijesti' + $t('apiS')]) }}</div>\n        </div>\n        </div>\n        \n        <div id=\"endline\"></div>\n        </a>\n    </div>\n</template>\n<script>\nimport store from '../store'\nimport ArticleComponent from './ArticleComponent.vue'\nimport { useI18n } from 'vue-i18n'\nimport removeMarkdown from \"markdown-to-text\";\nexport default {\n  components: { \n      ArticleComponent,\n      \n      },\n    name: \"NewsElement\",\n    data: function(){\n      return {\n          store,\n          \n      }\n  },\n    props: {\n        aitem: Object\n    },\n    methods: {\n        formatDate(dateString) {\n            const { t } = useI18n({});\n            const date = new Date(dateString);\n            return new Intl.DateTimeFormat(t('datelang'), {dateStyle: 'long'}).format(date);\n        },\n        removeMarks(contString) {\n            const conte = contString\n            return removeMarkdown(conte)\n        }\n    }\n}\n</script>\n<style scoped>\na {\n    text-decoration: none;\n}\n#newscont {\n   display: -webkit-box;\n   margin-top: 0.2vw;\n   line-height: 1;\n   -webkit-line-clamp: 2;\n   -webkit-box-orient: vertical;\n   overflow: hidden;\n   font-size: 1rem;\n   color: rgb(4, 31, 48);\n       font-weight: 600;\n    font-family: 'Helvetica-Medium';\n   text-align: left;\n   \n}\n#newsnaslov {\n        height: max-content;\n    display: -webkit-box;\n    -webkit-line-clamp: 1;\n    \n    -webkit-box-orient: vertical;\n    overflow: hidden;\n    text-align: left;